{
  "name": "rubicon",
  "version": "0.0.1",
  "description": "Launcher for TES4:Oblivion and game builds based on it",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EdMSL/rubicon.git"
  },
  "author": "Eduard Trayt <eduard2011rus@gmail.com> (https://github.com/EdMSL)",
  "license": "MIT",
  "scripts": {
    "start": "cross-env electron ./app/build",
    "build": "rm -rf ./app/build/* && cross-env NODE_ENV=production webpack --env production --config=./config/webpack.build.config.js --progress",
    "dev": "cross-env webpack serve --config ./config/webpack.dev.config.js",
    "stylelint": "stylelint src/**/*.scss --config ./.stylelintrc-full.js --syntax scss --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "4.21.0",
    "@typescript-eslint/parser": "4.21.0",
    "cross-env": "7.0.3",
    "css-loader": "5.2.0",
    "electron": "12.0.2",
    "eslint": "7.23.0",
    "html-webpack-plugin": "5.3.1",
    "mini-css-extract-plugin": "1.4.0",
    "sass-loader": "11.0.1",
    "sass-resources-loader": "2.1.1",
    "style-loader": "2.0.0",
    "stylelint": "13.12.0",
    "stylelint-at-rule-no-children": "0.3.1",
    "stylelint-config-recommended": "4.0.0",
    "stylelint-declaration-use-variable": "1.7.2",
    "stylelint-order": "4.1.0",
    "stylelint-scss": "3.19.0",
    "ts-loader": "8.1.0",
    "ts-node": "9.1.1",
    "typescript": "4.2.3",
    "webpack": "5.30.0",
    "webpack-cli": "4.6.0",
    "webpack-dev-server": "3.11.2",
    "webpack-merge": "5.7.3"
  }
}
