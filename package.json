{
  "name": "rocket-game-launcher",
  "version": "2.0.0",
  "description": "A program for launching games and managing game builds based on them ",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EdMSL/rocket.git"
  },
  "private": true,
  "author": "Eduard Trayt <eduard2011rus@gmail.com> (https://github.com/EdMSL)",
  "license": "MIT",
  "scripts": {
    "start": "npm run dev:main && npm-run-all --parallel dev:renderer server",
    "server": "cross-env electron ./app/build",
    "dev:main": "cross-env webpack --env main --config ./config/webpack.dev.config.js",
    "dev:renderer": "cross-env FAST_REFRESH=true NODE_ENV=development webpack serve --config ./config/webpack.dev.config.js",
    "build": "rm -rf ./app/dist/* && npm run build:main && npm run build:renderer",
    "build:main": "cross-env NODE_ENV=production webpack --env main --config=./config/webpack.build.config.js --progress",
    "build:renderer": "cross-env NODE_ENV=production webpack --config=./config/webpack.build.config.js --progress",
    "pack": "node scripts/pack.js --asar",
    "pack:test": "node scripts/pack.js",
    "release": "npm run build && npm run pack",
    "release:test": "npm run build && npm run pack:test",
    "stylelint": "stylelint src/**/*.scss --config ./.stylelintrc-full.js --custom-syntax postcss-scss --fix",
    "eslint": "eslint --config ./.eslintrc.js src --ext .ts,.tsx,.js,.jsx --fix",
    "eslint-nofix": "eslint --config ./.eslintrc.js src --ext .ts,.tsx,.js,.jsx",
    "tslint": "bash -c 'tsc --noEmit --project tsconfig.json'",
    "lint": "npm run stylelint && npm run eslint && npm run tslint",
    "test": "mocha --config ./.mocharc.js",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{css,scss}": [
      "npm run stylelint"
    ],
    "*.{js,jsx}": [
      "npm run eslint"
    ],
    "*.{ts,tsx}": [
      "npm run tslint",
      "npm run eslint"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.17.5",
    "@babel/plugin-syntax-top-level-await": "7.14.5",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.4",
    "@redux-devtools/extension": "3.2.2",
    "@types/chai": "4.2.21",
    "@types/mime": "2.0.3",
    "@types/mocha": "9.0.0",
    "@types/mock-fs": "4.13.1",
    "@types/node": "17.0.21",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.3",
    "@types/react-redux": "7.1.16",
    "@types/react-router-dom": "5.1.7",
    "@types/webpack-env": "1.16.2",
    "@typescript-eslint/eslint-plugin": "4.21.0",
    "@typescript-eslint/parser": "4.21.0",
    "babel-loader": "8.2.3",
    "chai": "4.3.4",
    "classnames": "2.3.1",
    "cross-env": "7.0.3",
    "css-loader": "5.2.0",
    "electron": "17.4.1",
    "electron-packager": "15.4.0",
    "electron-unhandled": "4.0.1",
    "eslint": "7.23.0",
    "eslint-config-airbnb-typescript": "12.3.1",
    "eslint-import-resolver-webpack": "0.13.1",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "fast-xml-parser": "3.19.0",
    "html-webpack-plugin": "5.3.1",
    "husky": "7.0.4",
    "iconv-lite": "0.6.3",
    "ini-api": "1.2.0",
    "joi": "17.6.0",
    "lint-staged": "11.1.2",
    "mime": "2.5.2",
    "mini-css-extract-plugin": "1.4.0",
    "mocha": "^9.2.2",
    "mock-fs": "5.0.0",
