{
  "name": "rubicon",
  "version": "0.0.1",
  "description": "Launcher for TES4:Oblivion and game builds based on it",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EdMSL/rubicon.git"
  },
  "author": "Eduard Trayt <eduard2011rus@gmail.com> (https://github.com/EdMSL)",
  "license": "MIT",
  "scripts": {
    "start": "npm run dev:main && npm-run-all --parallel dev:renderer server",
    "server": "cross-env electron ./app/build",
    "dev:main": "cross-env webpack --env main --config ./config/webpack.dev.config.js",
    "dev:renderer": "cross-env NODE_ENV=development webpack serve --config ./config/webpack.dev.config.js",
    "build": "rm -rf ./app/build/* && npm run build:main && npm run build:renderer",
    "build:main": "cross-env NODE_ENV=production webpack --env main --config=./config/webpack.build.config.js --progress",
    "build:renderer": "cross-env NODE_ENV=production webpack --config=./config/webpack.build.config.js --progress",
    "pack": "cross-env electron-packager ./app 'OA_Launcher' --out='./release' --platform='win32' --arch='ia32' --overwrite --prune --asar --ignore='/src' --icon=src/public/icon.ico",
    "pack:test": "cross-env electron-packager ./app 'OA_Launcher' --out='./release' --platform='win32' --arch='ia32' --overwrite --prune --ignore='/src' --icon=src/public/icon.ico",
    "release": "npm run build && npm run pack",
    "release:test": "npm run build && npm run pack:test",
    "stylelint": "stylelint src/**/*.scss --config ./.stylelintrc-full.js --syntax scss --fix",
    "eslint": "eslint --config ./.eslintrc.js src --ext .ts,.tsx,.js,.jsx --fix",
    "eslint-nofix": "eslint --config ./.eslintrc.js src --ext .ts,.tsx,.js,.jsx",
    "tslint": "tsc --project tsconfig.json",
    "lint": "npm run stylelint && npm run eslint && npm run tslint",
    "test": "mocha --no-package --config ./.mocharc.js"
  },
  "devDependencies": {
    "@types/chai": "4.2.21",
    "@types/mocha": "9.0.0",
    "@types/mock-fs": "4.13.1",
    "@types/node": "16.4.11",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.3",
    "@types/react-redux": "7.1.16",
    "@types/react-router-dom": "5.1.7",
    "@types/webpack-env": "1.16.2",
    "@typescript-eslint/eslint-plugin": "4.21.0",
    "@typescript-eslint/parser": "4.21.0",
    "chai": "4.3.4",
    "classnames": "2.3.1",
    "cross-env": "7.0.3",
    "css-loader": "5.2.0",
    "electron": "13.1.8",
    "electron-packager": "^15.3.0",
    "eslint": "7.23.0",
    "eslint-config-airbnb-typescript": "12.3.1",
    "eslint-import-resolver-webpack": "0.13.1",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "html-webpack-plugin": "5.3.1",
    "iconv-lite": "0.6.3",
    "mini-css-extract-plugin": "1.4.0",
    "mocha": "9.0.3",
    "mock-fs": "5.0.0",
    "npm-run-all": "4.1.5",
    "reduxsauce": "1.2.0",
    "sass": "1.32.8",
    "sass-loader": "11.0.1",
    "sass-resources-loader": "2.1.1",
    "style-loader": "2.0.0",
    "stylelint": "13.13.1",
    "stylelint-at-rule-no-children": "0.3.1",
    "stylelint-config-recommended": "4.0.0",
    "stylelint-config-standard": "22.0.0",
    "stylelint-declaration-use-variable": "1.7.3",
    "stylelint-order": "4.1.0",
    "stylelint-scss": "3.19.0",
    "ts-loader": "8.1.0",
    "ts-node": "9.1.1",
    "typescript": "4.2.3",
    "webpack": "5.49.0",
    "webpack-cli": "4.7.2",
    "webpack-dev-server": "3.11.2",
    "webpack-merge": "5.7.3"
  },
  "dependencies": {
    "@electron/remote": "1.1.0",
    "axios": "0.21.1",
    "connected-react-router": "6.9.1",
    "electron-redux": "1.5.4",
    "electron-store": "7.0.3",
    "electron-window-state": "5.0.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.3",
    "react-router-dom": "5.2.0",
    "redux": "4.0.5",
    "redux-saga": "1.1.3"
  }
}
